    </servlet>
    <servlet>
        <servlet-name>NuevoUsuarioServlet</servlet-name>
        <servlet-class>controlador.NuevoUsuarioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CerrarSesionServlet</servlet-name>
        <servlet-class>controlador.CerrarSesionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditarUsuarioServlet</servlet-name>
        <servlet-class>controlador.EditarUsuarioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RecordatorioServlet2</servlet-name>
        <servlet-class>controlador.RecordatorioServlet2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>IndexServlet</servlet-name>
        <url-pattern>/IndexServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RecordatorioServlet</servlet-name>
        <url-pattern>/RecordatorioServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletBB</servlet-name>
        <url-pattern>/ServletBB</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditarClienteServlet</servlet-name>
        <url-pattern>/EditarClienteServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListarClienteServlet</servlet-name>
        <url-pattern>/ListarClienteServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NuevoClienteServlet</servlet-name>
        <url-pattern>/NuevoClienteServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BuscarRecordatorioServlet</servlet-name>
        <url-pattern>/BuscarRecordatorioServlet</url-pattern>
    </servlet-mapping>
