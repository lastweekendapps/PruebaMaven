<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
    version="3.0">
    <display-name>CrunchifyJSPServletExample</display-name>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>IndexServlet</servlet-name>
        <servlet-class>controlador.IndexServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RecordatorioServlet</servlet-name>
        <servlet-class>controlador.RecordatorioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletBB</servlet-name>
        <servlet-class>controlador.ServletBB</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditarClienteServlet</servlet-name>
        <servlet-class>controlador.EditarClienteServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListarClienteServlet</servlet-name>
        <servlet-class>controlador.ListarClienteServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NuevoClienteServlet</servlet-name>
        <servlet-class>controlador.NuevoClienteServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BuscarRecordatorioServlet</servlet-name>
